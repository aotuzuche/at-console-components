{
  "name": "at-console-components",
  "version": "1.0.0",
  "main": "./lib/index",
  "module": "./es/index",
  "typings": "lib/index.d.ts",
  "repository": "git@github.com:aotuzuche/at-console-components.git",
  "author": "lixuingyun2010@gmail.com",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env webpack-dev-server --host 0.0.0.0 --config scripts/webpack.config.dev.js",
    "build": "yarn clean && yarn build:lib && yarn build:es",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./.babelrc components --out-dir es --ignore **/*.test.tsx,**/demo/* --extensions \".ts,.tsx\" && tsc -p tsconfig.json --outDir es",
    "build:lib": "cross-env NODE_ENV=production babel --config-file ./.babelrc components --out-dir lib --ignore **/*.test.tsx,**/demo/* --extensions \".ts,.tsx\" && tsc -p tsconfig.json --outDir lib",
    "clean": "rimraf es && rimraf lib",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:tsc": "tsc --noEmit"
  },
  "files": [
    "lib",
    "es"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": "^16.13.1"
  },
  "dependencies": {
    "@ant-design/compatible": "^1.0.2",
    "@ant-design/icons": "^4.1.0",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-router-dom": "^5.1.5",
    "antd": "^4.1.5",
    "auto-libs": "^0.1.82",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "dva-core": "^1.4.0",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-use": "^14.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/lodash": "^4.14.150",
    "@types/lodash-es": "^4.17.3",
    "@types/mockjs": "^1.0.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-atzuche": "^0.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^6.7.2",
    "eslint-config-atzuche": "^0.0.15",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.0.0",
    "fork-ts-checker-notifier-webpack-plugin": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^3.1.0",
    "lint-staged": "^10.0.7",
    "minimist": "^1.2.0",
    "mockjs": "^1.1.0",
    "node-sass": "^4.14.0",
    "prettier": "^1.18.2",
    "qs": "^6.7.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.0",
    "typescript": "^3.6.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "yamljs": "^0.3.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
